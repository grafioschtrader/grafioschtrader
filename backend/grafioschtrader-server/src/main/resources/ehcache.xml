<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd">
	<defaultCache maxElementsInMemory="100" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120"
		memoryStoreEvictionPolicy="LRU">
		<persistence strategy="localTempSwap" />
	</defaultCache>
	<cache name="grafioschtrader.entities.Assetclass"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>
	<cache name="grafioschtrader.entities.Globalparameters"
		maxElementsInMemory="30" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>

	<cache name="grafioschtrader.entities.ImportTransactionPlatform"
		maxElementsInMemory="30" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>


	<cache name="grafioschtrader.entities.MultilanguageString"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>

	<cache name="grafioschtrader.entities.MultilanguageString.map"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>
	<cache name="grafioschtrader.entities.Role"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU">
	</cache>

	<cache name="grafioschtrader.entities.User.roles"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU">
	</cache>

	<cache name="grafioschtrader.entities.Stockexchange"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" memoryStoreEvictionPolicy="LRU">
	</cache>


	<cache name="grafioschtrader.entities.User"
		maxElementsInMemory="100" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU">
	</cache>

</ehcache>